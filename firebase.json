{
  "hosting": [
    {
      "target": "dev",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "test",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    },
    {
      "target": "prod",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "/sitemap-article.xml",
          "function": "sitemap"
        },
        {
          "source": "/sitemap-article-1.xml",
          "function": "sitemapArticle1"
        },
        {
          "source": "/sitemap-article-2.xml",
          "function": "sitemapArticle2"
        },
        {
          "source": "/sitemap-article-3.xml",
          "function": "sitemapArticle3"
        },
        {
          "source": "/sitemap-article-4.xml",
          "function": "sitemapArticle4"
        },
        {
          "source": "/rss",
          "function": "feedRssAll"
        },
        {
          "source": "**",
          "function": "ssr"
        }
      ],
      "redirects": [
        {
          "source": "/fr/profil/annie-japaud",
          "destination": "/annie-japaud",
          "type": 301
        },
        {
          "source": "/fr/profil/april-soller",
          "destination": "/april-soller",
          "type": 301
        },
        {
          "source": "/en/profile/franki-osegueda/?l=es",
          "destination": "/franki-osegueda",
          "type": 301
        },
        {
          "source": "/es/videos/the-persuit-of-happiness",
          "destination": "/shawn-engel/the-pursuit-of-happiness",
          "type": 301
        },
        {
          "source": "/es/videos/strawberry-coconut-balls",
          "destination": "/ana-najdovska/strawberry-coconut-bites",
          "type": 301
        },
        {
          "source": "/en/profile/stephanie-effendie/?l=fr",
          "destination": "/stephanie-effendie",
          "type": 301
        },
        {
          "source": "/es/perfil/kylee-ellis/?l=es",
          "destination": "/kylee-ellis",
          "type": 301
        },
        {
          "source": "/en/profile/leah-fleurimond/?l=es",
          "destination": "/leah-fleurimond",
          "type": 301
        }
      ],
      "headers": [
        {
          "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            },
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        },
        {
          "source": "**/*@(manifest.json|manifest.webmanifest)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        },
        {
          "source": "**/*.@(jpg|jpeg|gif|png)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        },
        {
          "source": "**/*.@(js|map|css|json)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        }
      ]
    },
    {
      "target": "preprod",
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "ssr"
        }
      ],
      "headers": [
        {
          "source": "**/*.@(eot|otf|ttf|ttc|woff|woff2|font.css)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            },
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        },
        {
          "source": "**/*@(manifest.json|manifest.webmanifest)",
          "headers": [
            {
              "key": "Access-Control-Allow-Origin",
              "value": "*"
            }
          ]
        },
        {
          "source": "**/*.@(jpg|jpeg|gif|png)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        },
        {
          "source": "**/*.@(js|map|css|json)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=31536000"
            }
          ]
        }
      ]
    }
  ]
}